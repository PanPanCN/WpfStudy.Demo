<Window
    x:Class="AttchedPropertyGetViews.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AttchedPropertyGetViews"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    local:AttchedPropertys.GetView="True"
    DataContext="{Binding Source={StaticResource ResourceKey=locator}, Path=MainWindowViewModel}"
    mc:Ignorable="d">
    <StackPanel>
        <Button Command="{Binding ClickCommand}" Content="确定" />
    </StackPanel>
</Window>
